{
  "name": "whatthejwt",
  "displayName": "WhatTheJWT",
  "description": "Quickly translate Base64 encoded JWTs to their readable versions.",
  "version": "0.0.2",
  "publisher": "FirewireFahsel",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.whatTheJWT"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.whatTheJWT",
        "title": "Decode JWT"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "atob": "*"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "eslint": "^4.11.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
